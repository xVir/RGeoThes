<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema version="1.0" xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <xs:complexType name="record">
    <xs:sequence>
      <xs:element name="belongTo" type="recordReference" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="contains" type="recordReference" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="locations" type="location" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="names" type="name" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="previous" type="recordReference" minOccurs="0"/>
      <xs:element name="primaryName" type="name" minOccurs="0"/>
      <xs:element name="qualifier" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="recordReference">
    <xs:sequence>
      <xs:element name="document" type="document" minOccurs="0"/>
      <xs:element name="recordFromQualifier" type="xs:string" minOccurs="0"/>
      <xs:element name="recordToQualifier" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="document">
    <xs:sequence>
      <xs:element name="creationDate" type="xs:dateTime" minOccurs="0"/>
      <xs:element name="date" type="xs:dateTime" minOccurs="0"/>
      <xs:element name="description" type="xs:string" minOccurs="0"/>
      <xs:element name="uri" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="location" abstract="true">
    <xs:sequence>
      <xs:element name="beginDocument" type="document" minOccurs="0"/>
      <xs:element name="endDocument" type="document" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="name">
    <xs:sequence>
      <xs:element name="beginDocument" type="document" minOccurs="0"/>
      <xs:element name="endDocument" type="document" minOccurs="0"/>
      <xs:element name="language" type="xs:string" minOccurs="0"/>
      <xs:element name="name" type="xs:string" minOccurs="0"/>
      <xs:element name="type" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="point">
    <xs:complexContent>
      <xs:extension base="location">
        <xs:sequence>
          <xs:element name="latitude" type="xs:double"/>
          <xs:element name="longitude" type="xs:double"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="rectangle">
    <xs:complexContent>
      <xs:extension base="location">
        <xs:sequence>
          <xs:element name="point1" type="point" minOccurs="0"/>
          <xs:element name="point2" type="point" minOccurs="0"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
</xs:schema>

